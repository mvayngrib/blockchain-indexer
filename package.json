{
  "name": "blockchain-indexer",
  "version": "1.0.0",
  "description": "indexer for ethereum blockchain to map addrs -> txs",
  "main": "./lib/index.js",
  "bin": "./lib/cli/index.js",
  "scripts": {
    "test": "node lib/test",
    "lint": "eslint --ignore-path .gitignore --quiet .",
    "precommit": "npm run lint && npm test",
    "parity:ropsten": "CHAIN=ropsten docker-compose -f ./docker/docker-compose-parity.yml up -d",
    "parity:ropsten:stop": "CHAIN=ropsten docker-compose -f ./docker/docker-compose-parity.yml stop",
    "parity:mainnet": "CHAIN=mainnet docker-compose -f ./docker/docker-compose-parity.yml up -d",
    "parity:mainnet:stop": "CHAIN=mainnet docker-compose -f ./docker/docker-compose-parity.yml stop",
    "geth:ropsten": "NETWORK_ID=3 CHAIN=ropsten docker-compose -f ./docker/docker-compose-geth.yml up -d",
    "geth:ropsten:stop": "NETWORK_ID=3 CHAIN=ropsten docker-compose -f ./docker/docker-compose-geth.yml stop",
    "geth:rinkeby": "docker-compose -f ./docker/docker-compose-geth-rinkeby.yml up -d",
    "geth:rinkeby:stop": "docker-compose -f ./docker/docker-compose-geth-rinkeby.yml stop",
    "geth:mainnet": "HOST_IP=$(ipconfig getifaddr en0) NETWORK_ID=1 CHAIN=mainnet docker-compose -f ./docker/docker-compose-geth.yml up -d",
    "geth:mainnet:stop": "NETWORK_ID=1 CHAIN=mainnet docker-compose -f ./docker/docker-compose-geth.yml stop",
    "get:height": "node scripts/web3.js getBlockNumber",
    "start": "./lib/cli/index.js start"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "indexes"
  ],
  "author": "mvayngrib",
  "license": "MIT",
  "dependencies": {
    "@tradle/errors": "^1.1.1",
    "bluebird": "^3.5.1",
    "event-hooks": "^1.2.0",
    "eventemitter2": "^5.0.1",
    "koa": "^2.5.2",
    "koa-router": "^7.4.0",
    "level": "^4.0.0",
    "levelup": "^3.1.1",
    "lodash": "^4.17.10",
    "pify": "^3.0.0",
    "protocol-buffers": "^4.0.4",
    "pump": "^3.0.0",
    "redis": "^2.8.0",
    "stream-collector": "^1.0.1",
    "subleveldown": "^3.0.1",
    "through2": "^2.0.3",
    "traverse": "^0.6.6",
    "web3-eth": "^1.0.0-beta.35",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "ganache-cli": "^6.1.6",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "memdown": "^3.0.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.2.0",
    "standard-version": "^4.4.0",
    "tape": "^4.9.1"
  }
}
